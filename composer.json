{
    "name": "slim/slim-gw",
    "description": "A Slim Framework skeleton application for rapid development",
    "keywords": ["microframework", "rest", "router", "psr7"],
    "homepage": "http://github.com/anonimak",
    "license": "unlicense",
    "authors": [
        {
            "name": "Jonatan Teofilus",
            "email": "jonatan.teofilus@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "ext-json": "*",
        "faapz/pdo": "^1.1",
        "monolog/monolog": "^1.24",
        "php-di/php-di": "^6.0",
        "slim/psr7": "^0.4.0",
        "slim/slim": "^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "process-timeout": 0,
        "sort-packages": true
    },
    "scripts": {
        "start": "php -S 1.1.1.115:8080 -t public",
        "test": "phpunit"
    }
}
